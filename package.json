{
  "name": "@vincentgraul/react-components",
  "version": "1.0.3",
  "main": "esm/index.js",
  "module": "cjs/index.js",
  "exports": {
    "./match-device": {
      "import": "./esm/match-device/useMatchDevice.js",
      "require": "./cjs/match-device/useMatchDevice.js"
    },
    "./match-resolution": {
      "import": "./esm/match-resolution/useMatchResolution.js",
      "require": "./cjs/match-resolution/useMatchResolution.js"
